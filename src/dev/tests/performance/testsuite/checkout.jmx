          <stringProp name="script">var numAfter = vars.get(&apos;numOrders&apos;);
if (numAfter !== null) { // The asserion is called for every sampler, but we want it to act only after orders number is retrieved
    var numBefore = props.get(&apos;numOrdersBefore&apos;);
    var numCreated = numAfter - numBefore;
    var numExpected = vars.get(&apos;USERS&apos;) * vars.get(&apos;LOOPS&apos;);

    if (numCreated != numExpected) {
        var message = &apos;Expected number of created orders: &apos; + numExpected + &apos;, actual: &apos; + numCreated + &apos; (total before: &apos; +  numBefore + &apos;, total after: &apos; + numAfter + &apos;)&apos;;
        AssertionResult.setFailureMessage(message);
