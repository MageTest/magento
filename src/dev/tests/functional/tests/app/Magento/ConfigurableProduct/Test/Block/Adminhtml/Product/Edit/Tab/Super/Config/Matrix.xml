<mapping strict="0">
    <fields>
        <display>
            <selector>[name="associated_product_ids[]"]</selector>
            <input>checkbox</input>
        </display>
        <name>
            <selector>[name$="[name]"]</selector>
        </name>
        <sku>
            <selector>[name$="[sku]"]</selector>
        </sku>
        <quantity_and_stock_status composite="1">
            <qty>
                <selector>[name$="[quantity_and_stock_status][qty]"]</selector>
            </qty>
        </quantity_and_stock_status>
        <weight>
            <selector>[name$="[weight]"]</selector>
        </weight>
    </fields>
</mapping>

