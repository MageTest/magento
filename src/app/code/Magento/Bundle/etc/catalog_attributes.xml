    <group name="catalog_product">
        <attribute name="price_view"/>
        <attribute name="price_type"/>
    </group>
    <group name="sales_quote_item">
        <attribute name="price_view"/>
        <attribute name="price_type"/>
        <attribute name="shipment_type"/>
        <attribute name="weight_type"/>
