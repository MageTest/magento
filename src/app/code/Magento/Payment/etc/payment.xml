<payment xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:noNamespaceSchemaLocation="../../../Magento/Payment/etc/payment.xsd">
    <credit_cards>
        <type id="AE" order="0">
            <label>American Express</label>
        </type>
        <type id="VI" order="10">
            <label>Visa</label>
        </type>
        <type id="MC" order="20">
            <label>MasterCard</label>
        </type>
        <type id="DI" order="30">
            <label>Discover</label>
        </type>
        <type id="SM" order="40">
            <label>Maestro/Switch</label>
        </type>
        <type id="SO" order="45">
            <label>Solo</label>
        </type>
        <type id="JCB" order="50">
            <label>JCB</label>
        </type>
        <type id="OT" order="1000">
            <label>Other</label>
        </type>
    </credit_cards>
</payment>
